{
  "name": "adminservice",
  "version": "1.0.0",
  "description": "NestJS Admin microservice for role and stock management",
  "main": "dist/main.js",
  "scripts": {
    "generate": "prisma generate --schema prisma/schema.prisma",
    "build": "npm run generate && nest build --config tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=src --testPathIgnorePatterns=test",
    "test:integration": "jest --testPathPattern=src --testNamePattern=integration",
    "test:verbose": "jest --verbose",
    "test:all": "./scripts/run-tests.sh all"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.4.20",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^6.10.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.13",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "jest": "^29.5.0",
    "prisma": "^6.10.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.2"
  }
}
